{
  "name": "landing-page",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "concurrently \"npm:watch\" \"npm:serve\"",
    "serve": "snowpack dev",
    "watch": "concurrently \"npm:watch-sass\" \"npm:watch-html\"",
    "watch-sass": "sass --watch ./:./",
    "watch-html": "eleventy --watch",
    "build": "sass ./:./ && eleventy --pathprefix=landing-page && snowpack build"
  },
  "author": "",
  "license": "GPL-3.0",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "concurrently": "^5.3.0",
    "http-server": "^0.12.3",
    "sass": "^1.32.5",
    "snowpack": "^3.0.13"
  },
  "dependencies": {
    "@codemirror/basic-setup": "^0.18.0",
    "@codemirror/commands": "^0.18.0",
    "@codemirror/lang-javascript": "^0.18.0",
    "@codemirror/state": "^0.18.0",
    "@codemirror/view": "^0.18.1"
  }
}
